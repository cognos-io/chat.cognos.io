{
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.2.2",
    "@angular/material-experimental": "^17.2.2",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@fontsource-variable/public-sans": "^5.0.17",
    "argon2id": "^1.0.1",
    "blakejs": "^1.2.1",
    "bootstrap-icons": "^1.11.3",
    "js-base64": "^3.7.7",
    "ngxtension": "^2.1.0",
    "pocketbase": "^0.21.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.22.4",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/localize": "^17.2.0",
    "@jest/globals": "^29.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.2",
    "util": "^0.12.5"
  },
  "name": "@cognos/chat",
  "prettier": {
    "importOrder": [
      "^@angular/(.*)$",
      "^pocketbase(.*)$",
      "^rxjs(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^@app/(.*)$",
      "^@components/(.*)$",
      "^@environments/(.*)$",
      "^@guards/(.*)$",
      "^@interfaces/(.*)$",
      "^@pages/(.*)$",
      "^@services/(.*)$",
      "^[./]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "decorators"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "printWidth": 88,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "private": true,
  "scripts": {
    "build": "ng build",
    "extract-i18n": "ng extract-i18n --output-path=src/locale --format=xlf2",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,scss,css,md}\"",
    "lint": "ng lint --fix",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "typegen": "pocketbase-typegen --db ../backend/pb_data/data.db --out ./src/app/types/pocketbase-types.ts",
    "watch": "ng build --watch --configuration development"
  },
  "version": "0.0.0"
}
